generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:database.db"
}

model Auth {
  id           Int    @id @default(autoincrement())
  passwordHash String
  agendaToken  String
}

model Birthdate {
  id        String @id
  name      String
  day       Int
  month     Int
  birthYear Int?
}
